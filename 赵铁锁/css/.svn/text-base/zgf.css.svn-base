.nav {
  width: 100%;
  height: 70px;
  background: #333333;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}
.nav .nav_n {
  width: 1280px;
  height: 70px;
  margin: 0 auto;
  position: relative;
}
.nav .nav_n .dl {
  font-size: 12px;
  position: absolute;
  right: 20px;
  top: 5px;
  z-index: 6;
  color: #959594;
}
.nav .nav_n .dl a {
  padding: 0 10px;
  color: #959594;
}
.nav .nav_n .dl a:hover {
  color: #FFFFFF;
}
.nav .nav_n .nav_l {
  float: left;
  margin-left: 35px;
}
.nav .nav_n .nav_r {
  float: right;
}
.nav .nav_n .nav_r ul {
  float: right;
}
.nav .nav_n .nav_r ul li {
  cursor: pointer;
  float: left;
  padding: 0 30px;
  height: 70px;
  line-height: 70px;
  font-size: 20px;
  font-weight: bold;
  text-align: center;
}
.nav .nav_n .nav_r ul li a {
  display: block;
  color: #FFFFFF;
}
.nav .nav_n .nav_r ul li a:hover {
  color: #f75c10;
}
.nav .nav_n .nav_r ul .active a {
  color: #f00;
  font-family: "微软雅黑";
}
.nav .nav_n .nav_r ul .active a:hover {
  color: #f00;
}
.swiper-container {
  width: 100%;
  height: 100%;
}
.swiper-container .swiper-wrapper .swiper-slide {
  font-size: 0;
}
.swiper-container .swiper-wrapper .swiper-slide img {
  width: 100%;
  height: 100%;
}
.swiper-button-next {
  width: 18px;
  height: 38px;
  background: plum;
  display: none;
  opacity: 0.5;
}
.swiper-button-prev {
  width: 18px;
  height: 38px;
  background: plum;
  display: none;
  opacity: 0.5;
}
.dlk_bg {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: #333333;
  opacity: 0.8;
  z-index: 99999;
  display: none;
}
.dlk {
  width: 508px;
  height: 350px;
  background: #fae7ad;
  position: fixed;
  left: 100px;
  top: 90px;
  z-index: 999999;
  display: none;
}
.dlk h1 {
  width: 508px;
  height: 40px;
  background: #777777;
  line-height: 40px;
  padding: 0 20px;
  font-size: 16px;
  color: #FFFFFF;
}
.dlk h1 span {
  float: right;
  cursor: pointer;
}
.dlk .dlk_c {
  width: 508px;
  height: 310px;
  padding: 60px 100px;
}
.dlk .dlk_c table {
  font-size: 14px;
}
.dlk .dlk_c table tr {
  width: 100%;
  height: 30px;
  line-height: 30px;
  padding-bottom: 60px;
  color: #CE3A00;
}
.dlk .dlk_c table tr input {
  width: 200px;
  height: 26px;
  padding-left: 10px;
  outline: none;
  border: 1px solid #E67E39;
  background: #fbeec6;
}
.dlk .dlk_c table tr .le {
  text-align: right;
  color: #CE3A00;
}
.dlk .dlk_c table tr .re {
  text-align: right;
  font-size: 12px;
}
.dlk .dlk_c table tr .re a {
  color: #CE3A00;
  font-size: 12px;
  padding: 0 8px;
}
.dlk .dlk_c table tr .dl_btn {
  height: 35px;
  color: #FFFFFF;
  border: none;
  background: #000000;
  cursor: pointer;
}
