@w:100%;
@h:100%;


*{margin: 0;padding: 0;box-sizing: border-box;}
ul{list-style: none;}
a{text-decoration: none;}
body{padding-top: 70px; overflow: hidden;}
@font-face{
    font-family: ffs;
    src: url(../font/书体坊兰亭体.TTF);
}
/*通用结束*/


.nav{
    width: 100%;
    height: 70px;
    background: #333333;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;
    .nav_n{
        width: 1280px;
        height: 70px;
        margin: 0 auto;
        .nav_l{
            float: left;
            margin-left: 35px;
        }
        .nav_r{
            float: right;
            ul{
                float: right;
                li{
                    cursor: pointer; 
                    float: left;
                    padding: 0 30px;
                    height: 70px;
                    line-height: 70px;
                    font-size: 20px;
                    font-weight: bold;
                    text-align: center;
                    a{
                        display: block;
                        color: #FFFFFF;
                        &:hover{
                            color: #f75c10;
                        }
                    }
                }
                .active{
                    a{
                        color: #f00;
                        font-family: "微软雅黑";
                        &:hover{
                            color: #f00;
                        }
                    }
                } 
            }
        }
    }
}
.xiang{
    width: @w;
    height: @h;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 101;
    .icon1{
        position: absolute;
        left: 25%;
        top: 25%;
        text-align: center;
        .sh{
            display: block;
            transform: scale(1);
            transition: all 2s;
            width: 140px;
            height: 140px;
            border-radius: 120px;
            overflow: hidden;
            border: 2px solid #F75C10;
            img{
                transform: scale(1);
                transition: all 2s; 
            }
        }
        .title{
